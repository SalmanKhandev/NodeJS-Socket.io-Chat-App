								<div class="column width-5 offset-1">
									<div class="signup-box box rounded xlarge mb-0 bkg-white shadow">
										<h3>Register an Account</h3>
											<% if(hasErrors)  { %>
											<div class="box dismissable rounded small warning">
												<a href="#" class="dismissable"></a>
												<% _.forEach(messages,function(error){ %>
													<%= error %><br>
												<% }) %>
											</div>
										<% } %>
										<p class="mb-20">Already have an account? <a href="login" class="fade-location">Click Here</a></p>
										<div class="register-form-container">
											<form class="register-form" action="/signup" method="post">
												<div class="row">
													<!-- <div class="column width-6">
														<div class="field-wrapper">
															<label class="color-charcoal">First Name:</label>
															<input type="text" name="" class="form-firstname form-element rounded medium" placeholder="John" required>
														</div>
													</div> -->
													<div class="column width-12">
														<div class="field-wrapper">
															<label  for="username" class="color-charcoal">username:</label>
															<input type="text" name="username" class="form-element rounded medium" placeholder="zuber" >
														</div>
													</div>
												</div>
												<div class="row">
													<div class="column width-12">
														<div class="field-wrapper">
															<label  for="email"class="color-charcoal">Email:</label>
															<input type="email" name="email" class="form-element rounded medium" placeholder="zuber1077@yahoo.com" >
														</div>
													</div>
													<div class="column width-12">
														<div class="field-wrapper">
															<label for="password"class="color-charcoal">Password:</label>
															<input type="password" name="password" class="form-element rounded medium" placeholder="•••••••• (8 or more characters)" >
														</div>
													</div>
													<div class="column width-12 mt-10 center">
														<input type="submit" value="Register" class="button rounded medium bkg-green bkg-hover-theme bkg-focus-green color-white color-hover-white no-margins">
													</div>
												</div>
											</form>
											<p class="text-small mt-20 center">By creating an account you agree to our terms - <a href="#">Read More</a></p>
											<div class="form-response show"></div>
										</div>
									</div>
